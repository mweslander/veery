{
  "name": "veery",
  "version": "0.0.1",
  "description": "Perform tonight.",
  "main": "index.js",
  "scripts": {
    "build": "webpack -d",
    "build:production": "NODE_ENV=production webpack --colors --progress",
    "client:start": "webpack-dev-server -d --hot --port 8080",
    "client:test": "NODE_ENV=test mocha --require babel-register --opts 'support/mocha.opts' 'src/client/**/*.spec.js' --exit",
    "development": "npm run start:server & npm run start:client",
    "db:drop": "node src/api/database/drop.js",
    "db:drop:test": "NODE_ENV=test npm run db:drop",
    "db:furnish": "node src/api/database/furnish",
    "db:nightly-script": "node src/api/database/nightlyScript",
    "db:seed:admins": "node src/api/database/seedAdmins",
    "db:seed:events": "node src/api/database/seedEvents",
    "db:seed:review-app": "npm run db:seed:admins && npm run db:seed:venues && npm run db:seed:events",
    "db:seed:venues": "node src/api/database/seedVenues",
    "heroku-postbuild": "npm run build:production",
    "lint": "eslint ./src",
    "start": "node src/api/server.js",
    "server:start": "nodemon --watch src/api src/api/server.js",
    "server:test": "NODE_ENV=test npm run db:drop:test && NODE_ENV=test mocha --opts 'support/mocha.opts' 'src/api/**/*.spec.js' --exit",
    "test": "npm run server:test && npm run client:test"
  },
